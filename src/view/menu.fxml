<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="src.controller.menuController">
   <children>
      <Button fx:id="clientsButton" layoutX="261.0" layoutY="115.0" mnemonicParsing="false" onAction="#clientsMenu" text="Clients">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="appointmentsButton" layoutX="231.0" layoutY="180.0" mnemonicParsing="false" onAction="#appointmentsMenu" text="Appointments">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="reportsButton" layoutX="257.0" layoutY="254.0" mnemonicParsing="false" onAction="#reportsMenu" text="Reports">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="exitButton" layoutX="544.0" layoutY="356.0" mnemonicParsing="false" onAction="#exitButtonClick" text="Exit">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Label layoutX="248.0" layoutY="14.0" text="MAIN MENU">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
